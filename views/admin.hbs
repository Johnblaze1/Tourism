<!-- MAIN CONTENT -->
<div class="main-content">
    <div class="section__content section__content--p30">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-6">
                    <!-- PROFILE PHOTO -->
                    <div class="card">
                        <div class="card-header">
                            <strong class="card-title mb-3">Profile</strong>
                        </div>
                        <div class="card-body">
                            <div class="mx-auto d-block">
                                <img class=" mx-auto d-block" src="images/icon/avatar-01.jpg" alt="Card image cap">
                                <h5 class="text-sm-center mt-2 mb-1">ADMIN</h5>
                                <div class="location text-sm-center">
                                    <i class="fa fa-map-marker"></i>{{division}}</div>
                            </div>
                            <hr>

                        </div>
                    </div>

                </div>


                <div class="col-md-6">
                    <!-- PROFILE INFO -->
                    <div class="card">
                        <div class="card-header">
                            <strong class="card-title">INFORMATION</strong>
                        </div>
                        <div class="card-body">
                            <div class="sufee-alert alert with-close alert-primary alert-dismissible fade show">
                                <span class="badge badge-pill badge-primary">ADMIN USERNAME</span>
                                {{adminUsername}}

                            </div>

                            <div class="sufee-alert alert with-close alert-primary alert-dismissible fade show">
                                <span class="badge badge-pill badge-primary">CATEGORY</span>
                                ADMIN

                            </div>
                            <div class="sufee-alert alert with-close alert-primary alert-dismissible fade show">
                                <span class="badge badge-pill badge-primary">LAST LOGGED IN</span>
                                03-10-2019

                            </div>
                            <div class="sufee-alert alert with-close alert-primary alert-dismissible fade show">
                                <span class="badge badge-pill badge-primary">DATE CREATED</span>
                                {{date}}

                            </div>





                        </div>
                    </div>



                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="copyright">
                        <p>Copyright Â© 2019 All rights reserved <a href="https://regme.com">sageX</a>.</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
